<script>
    $(function(){
        $('.fix_erweima').hover(function(){
            $(this).find('.big').show();
        },function(){
            $(this).find('.big').hide();
        })
        if($(window).scrollTop()==0){
            $("#back_top").hide();
        }
        $(window).scroll(function(){       
            var ViewHeight = $(window).height();
            if($(window).scrollTop() >= (ViewHeight-10)){
                $("#back_top").show();
            }else{
                $("#back_top").hide();
            }
        });
        $("#back_top").click(function(){
            $('body,html').animate({scrollTop:0},50);
            return false;
        });        
    });
