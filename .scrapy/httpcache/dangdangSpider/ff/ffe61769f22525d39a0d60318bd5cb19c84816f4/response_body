    $("#__ddnav_sort16").remove();
}); 

$("#li_label_16").mouseover(function(){
	$("#li_label_16").addClass("on");
  $("#li_label_16").css("border-right","2px solid #ff2832");
  
});

$("#li_label_16").mouseout(function(){
	$("#li_label_16").removeClass("on");
