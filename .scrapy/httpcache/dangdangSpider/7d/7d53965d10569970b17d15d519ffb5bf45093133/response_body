    $(function(){
        try{ 
            var minsize=1210;
            var screensize=screen.width;
            if(screensize>minsize){//宽版
                if('pubAd' === 'pubAd'){
                    DD_AD.fetchCPT("top_50438","01.04.00.00.00.00",'2','1','1');
                }else{
                    DD_AD.fetchCPT("top_50438","1805","1","1","1");
                }
            }else{//窄版
                if('pubAd' === 'pubAd'){
                    DD_AD.fetchCPT("top_50438","01.04.00.00.00.00",'2','','1');
                }else{
                    DD_AD.fetchCPT("top_50438","1805","1","","1");
                }  
            }  
        } catch (e){
            
        }
    });
</script>
