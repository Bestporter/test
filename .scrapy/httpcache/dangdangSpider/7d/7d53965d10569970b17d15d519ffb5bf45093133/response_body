     for(var i=0; i<temp.length; i++){
         if(temp[i].indexOf('producthistoryid')!=-1 ){
             var exp = new Date();
             exp.setTime(exp.getTime()-400*24*3600*1000);
             document.cookie="producthistoryid=;expires="+exp.toGMTString()+";domain=dangdang.com;path=/";
