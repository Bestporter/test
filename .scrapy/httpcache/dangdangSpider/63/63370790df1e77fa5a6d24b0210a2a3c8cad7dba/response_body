            bindDDclick();
            page_tracker.trackPingPHP('http://search.dangdang.com/ping.php?'+'act=view&pos=0_0_0_p&cat=01.14.00.00.00.00&key=&qinfo=&pinfo=67069_1_60&minfo=&ninfo=&custid=&permid=&ref=&rcount=&type=&t=1588043927000&sell_run=0');
